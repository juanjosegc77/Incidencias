****** Comandos MySQL

CREATE TABLE alfabetico (id INTEGER(11) NOT NULL AUTO_INCREMENT PRIMARY KEY, adsc VARBINARY(80), tarjeta VARBINARY(80), numEmp VARBINARY(80), nombre VARBINARY(80), plaza VARBINARY(80), rfc VARBINARY(80), descpuesto VARBINARY(160), nombramiento VARBINARY(160), servicio VARBINARY(160), turno VARBINARY(80), entrada VARBINARY(80), salida VARBINARY(80), observaciones VARBINARY(320), fingreso VARBINARY(80));

CREATE TABLE vaclic (id_vaclic INTEGER(11) NOT NULL AUTO_INCREMENT PRIMARY KEY, id_alf INTEGER(11), folio VARBINARY(80), clave VARBINARY(80), periodoserie VARBINARY(80), dias VARBINARY(80), inicio VARBINARY(80), articulo VARBINARY(80), `1` VARBINARY(80), `2` VARBINARY(80), `3` VARBINARY(80), `4` VARBINARY(80), `5` VARBINARY(80), `6` VARBINARY(80), `7` VARBINARY(80), `8` VARBINARY(80), `9` VARBINARY(80), `10` VARBINARY(80), notas VARBINARY(320), fcaptura VARBINARY(80), hcaptura VARBINARY(80), FOREIGN KEY (id_alf) REFERENCES alfabetico(id)) ENGINE=INNODB;

CREATE TABLE Usuarios (Id_usuario INTEGER(11) NOT NULL AUTO_INCREMENT PRIMARY KEY, Usuario TEXT, Contrase√±a TEXT); 
